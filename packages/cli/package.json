{
	"name": "saihu",
	"version": "0.19.0",
	"description": "Client side chatbot framework that can be run in the command line",
	"author": "Fred Lin <gasolin+saihubot@gmail.com>",
	"license": "MIT",
	"bin": "dist/cli.js",
	"engines": {
		"node": ">=10"
	},
	"keywords": [
		"saihubot",
		"chatbot",
		"devops"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gasolin/saihubot.git"
	},
	"bugs": {
		"url": "https://github.com/gasolin/saihubot/issues"
	},
	"homepage": "https://github.com/gasolin/saihubot",
	"scripts": {
		"start": "babel-node --presets @babel/preset-env src/cli.js",
		"build": "babel --root-mode=upward ./src --out-dir ./dist --copy-files",
		"prepublish": "npm run build",
		"test": "xo && ava"
	},
	"dependencies": {
		"dedent": "^0.7.0",
		"execa": "^4.0.3",
		"import-jsx": "^4.0.0",
		"ink": "^3.0.8",
		"ink-markdown": "^1.0.1",
		"ink-select-input": "^4.2.0",
		"meow": "^7.1.1",
		"react": "^17.0.1",
		"saihubot": "^0.19.0",
		"saihubot-cli-adapter": "^0.19.0",
		"saihubot-skill-diagnostics": "^0.19.0",
		"saihubot-skill-search": "^0.19.0"
	},
	"devDependencies": {
		"@ava/babel": "^1.0.1",
		"@babel/cli": "^7.12.1",
		"@babel/core": "^7.12.3",
		"@babel/node": "^7.12.1",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.1",
		"@babel/register": "^7.12.1",
		"ava": "^3.13.0",
		"chalk": "^4.1.0",
		"eslint-config-xo-react": "^0.23.0",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"ink-testing-library": "^2.1.0",
		"xo": "^0.34.1"
	},
	"ava": {
		"babel": true,
		"require": [
			"@babel/register"
		]
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	}
}
