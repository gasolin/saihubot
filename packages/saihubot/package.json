{
  "name": "saihubot",
  "version": "0.38.0",
  "description": "Frontend framework to build message/chatbot ops, no server required.",
  "main": "dist/saihubot.js",
  "scripts": {
    "build": "babel --root-mode=upward ./src --out-dir ./dist --copy-files",
    "prepublish": "npm run build && npm run doc",
    "doc": "documentation build src -f html -o ../../docs/saihubot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gasolin/saihubot.git"
  },
  "keywords": [
    "saihubot",
    "hubot",
    "webbybot",
    "chatbot",
    "devops"
  ],
  "files": [
    "dist/**",
    "src/**"
  ],
  "author": "Fred Lin <gasolin+saihubot@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gasolin/saihubot/issues"
  },
  "homepage": "https://github.com/gasolin/saihubot/tree/gh-pages/packages/core#readme"
}
