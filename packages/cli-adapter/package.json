{
  "name": "saihubot-cli-adapter",
  "version": "0.37.0",
  "description": "Saihubot command line adapter with some bundled addons.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --root-mode=upward ./src --out-dir ./dist --copy-files",
    "prepublish": "npm run build && npm run doc",
    "doc": "documentation build src -f html -o ../../docs/saihubot-cli-adapter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gasolin/saihubot.git"
  },
  "keywords": [
    "saihubot-adapter",
    "saihubot-addon",
    "saihubot-skill"
  ],
  "files": [
    "dist/**",
    "src/**"
  ],
  "author": "Fred Lin <gasolin+saihubot@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gasolin/saihubot/issues"
  },
  "homepage": "https://github.com/gasolin/saihubot/tree/gh-pages/packages/cli-adapter#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "dedent": "^0.7.0",
    "execa": "^5.0.0",
    "ink": "^3.0.8",
    "ink-box": "^1.0.0",
    "ink-markdown": "^1.0.1",
    "ink-select-input": "^4.2.0",
    "meow": "^8.0.0",
    "node-fetch": "^3.1.1"
  },
  "peerDependencies": {
    "react": "^16.14.0"
  }
}
